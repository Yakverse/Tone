{
  "name": "tone-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && npm run build:js",
    "build:js": "babel src --out-dir dist --copy-files --extensions \".ts,.tsx\" --source-maps inline",
    "start:prod": "npm run build && cross-env NODE_ENV=production node ./dist/main.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@discordjs/builders": "^1.4.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.5.0",
    "@discordjs/voice": "^0.14.0",
    "@maroxy/ytfps": "^1.0.6",
    "@types/dotenv": "^8.2.0",
    "axios": "^1.3.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.37.33",
    "discord.js": "^14.7.1",
    "dotenv-flow": "^3.2.0",
    "https-proxy-agent": "^5.0.1",
