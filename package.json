{
  "name": "tone-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && npm run build:js",
    "build:js": "babel src --out-dir dist --copy-files --extensions \".ts,.tsx\" --source-maps inline",
    "start:prod": "npm run build && cross-env NODE_ENV=production node ./dist/main.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.10.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.4",
    "@maroxy/ytfps": "^1.0.6",
    "@types/dotenv": "^8.2.0",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.4.0",
    "dotenv-flow": "^3.2.0",
    "https-proxy-agent": "^5.0.0",
    "libsodium-wrappers": "^0.7.9",
    "play-dl": "^1.2.9",
    "rimraf": "^3.0.2",
    "soundcloud-downloader": "^1.0.0",
    "spdl-core": "^2.0.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "youtube-search-api": "^1.0.10",
    "ytdl-core": "4.10.0"
  }
}
