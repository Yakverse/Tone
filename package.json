    "soundcloud-downloader": "^1.0.0",
    "spdl-core": "^2.0.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "youtube-search-api": "^1.1.1",
    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "typescript": "^4.9.5"
  }
}
